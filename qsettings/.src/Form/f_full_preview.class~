' Gambas class file


Public Sub Form_Open()

  Me.FullScreen = True

End
Public Sub Form_KeyPress()
  
  If (key.Code = key.Esc) Then
    Me.Close
  Endif
  
End

Public Sub PicImgFondo_KeyPress()

   If (key.Code = key.Esc) Then
    Me.Close
  Endif

End

Public Sub PicImgFondo_MouseDown()

  Me.Close

End
